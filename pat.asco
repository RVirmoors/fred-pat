	@loopScreen
	curve playLoop
		@action := {cursor $x
			mugran positionvar ($x/500), period 0 (1 + $x/10000), periodvar ($x/500), duration 0 (1 + $x/15000), durationvar 0 ($x/15000), resampling ($x/50000*(@rand_int(200)-100)), levelvar ($x / 5000)
			$d[2] := $x/2000
			$a[2] := $x/200
		}
		@grain := 0.01
