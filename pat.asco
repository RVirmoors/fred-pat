	@toneScreen(vc 1, 0.70)

TRILL (A1) 2 ;m7
	@toneScreen(vc 1 vn1 1, 0.58)

	2 input-sel 1 ; VN1
NOTE 0 1
	@toneScreen(vc 1 va 1, 0.58)
	@resoScreen(vn1 1 vn2 1)
TRILL (F5 G6) 2 ;v1-improv
NOTE -F5 2 ;m8
