	input-sel 1 ; VN1
CHORD (F#5 G5) 1 ;vn1
NOTE F4 3 ;m12
	::spat_goto(1, -60, 4.5, 10)

NOTE G6 1 ;FL
NOTE F#5 1/2 ;fs
NOTE G5 1/2 ;g
NOTE G6 1 ;g_hi

NOTE D6 1 ;m13
