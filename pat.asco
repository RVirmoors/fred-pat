	curve 3-gain -50, 0.0	3	; fade in Grill
	2 ::spat_goto(3, 60, 1, 9)
	2 @noiseStop
	@noiseScreen(vn1 1 vn2 1 va 1.5 vc 1, 2)
	//curve 2-gain -30, 0.0	2.1	; fade in Noise
	$spatNoise := ::spat_radial(2)
	$spatTone := ::spat_radial(1)
