EVENT 11 P3m11
	::spat_goto(3, -155, 1, 4)

EVENT 9 P3m13
	::spat_goto(3, 15, 0.8, 1)

EVENT 25 P3m15
	::spat_goto(3, 333, 2, 15)

EVENT 45 P3-D

EVENT 155 P3-E

	1-gain 0
	3-gain 0
	spat room 1 size 1000
	spat room 1 reverberance 65
